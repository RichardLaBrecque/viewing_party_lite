<h1>Viewing Party Lite</h1>


<div class = "container">
  <% if !session[:user_id] %>
    <%= button_to "Create New User", '/register', method: :get %>
    <%= button_to "Log In", '/login', method: :get %>
  <% else %>
    <%= button_to "Log out", '/', method: :delete %>
    <div class="registered">
      <% @users.each do |user| %>
      <h3>name: <%= user.name %></h3>
      <p>email: <%= user.email %></p>
      <% end %>
    </div>
  <% end %>
</div>
